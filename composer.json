{
    "name": "ndobrovolsky/laravel-to-yap-validator",
    "description": "A package for converting Laravel validation rules to javascript validation rules, making it easy to reuse your existing validation rules and streamline your development process.",
    "keywords": [
        "Ndobrovolsky",
        "laravel-to-yap-validator"
    ],
    "homepage": "https://github.com/ndobrovolsky/laravel-to-yup-validator",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Nickolay Dobrovolsky",
            "email": "nickolaydobrovolsky@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ndobrovolsky\\LaravelToYapValidator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ndobrovolsky\\LaravelToYapValidator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ndobrovolsky\\LaravelToYapValidator\\LaravelToYapValidatorServiceProvider"
            ],
            "aliases": {
                "LaravelToYapValidator": "Ndobrovolsky\\LaravelToYapValidator\\LaravelToYapValidatorFacade"
            }
        }
    }
}
